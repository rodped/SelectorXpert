{
  "manifest_version": 3,
  "name": "SelectorXpert",
  "description": "Injecta e faz preview de estilos CSS via DevTools",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
